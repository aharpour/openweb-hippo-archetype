#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
#set($hyphen = '-')
#set($empty = '')
<?xml version="1.0" encoding="UTF-8"?>
<sv:node xmlns:sv="http://www.jcp.org/jcr/sv/1.0" xmlns:h="http://www.onehippo.org/jcr/xmlimport" sv:name="galleryProcessorService" h:merge="combine">
  <sv:property sv:name="jcr:primaryType" sv:type="Name">
    <sv:value>frontend:plugin</sv:value>
  </sv:property>
  <sv:node sv:name="${rootArtifactId.replace($hyphen,$empty)}:listSmallImage">
    <sv:property sv:name="jcr:primaryType" sv:type="Name">
      <sv:value>frontend:pluginconfig</sv:value>
    </sv:property>
    <sv:property sv:name="height" sv:type="Long">
      <sv:value>0</sv:value>
    </sv:property>
    <sv:property sv:name="upscaling" sv:type="Boolean">
      <sv:value>true</sv:value>
    </sv:property>
    <sv:property sv:name="width" sv:type="Long">
      <sv:value>60</sv:value>
    </sv:property>
  </sv:node>
</sv:node>
